print("   - Library:", tostring(Library ~= nil))
print("   - Window:", tostring(Window ~= nil))
print("   - Tabs:", tostring(Tabs ~= nil))

-- Auto-load Crafter Tab from GitHub
local function loadCrafterTab()
    local success, result = pcall(function()
        -- Use the correct GitHub raw URL
        local githubUrl = "https://raw.githubusercontent.com/bihyudyfosydf/Resn/refs/heads/main/Educatiion"

        local response = game:HttpGet(githubUrl)

        if response and #response > 0 then
            return loadstring(response)()
        else
            error("Empty response from GitHub")
        end
    end)

    if success and result then
        print("‚úÖ Crafter Tab loaded successfully!")
        Library:Notify("üî® Crafter Tab integrated! Check the new tab.", 3)
    else
        print("‚ö†Ô∏è Could not load Crafter Tab:", tostring(result))
        Library:Notify("‚ö†Ô∏è Crafter Tab failed to load - check console for details", 3)

        -- Additional debug info
        print("")
        print("")
    end
end

-- Auto-load Webhook Tab from GitHub or Local File
local function loadWebhookTab()
    local success, result = pcall(function()
        -- Ensure global variables are properly set for the webhook script
        if getgenv then
            getgenv().Library = Library
            getgenv().Window = Window
            getgenv().Tabs = Tabs
            getgenv().MainScriptLoaded = true
        end

        -- Make variables globally accessible without getgenv as backup
        _G.Library = Library
        _G.Window = Window
        _G.Tabs = Tabs
        _G.MainScriptLoaded = true
        _G.InitializeWebhookTab = true -- Flag for webhook script

        -- Try local file first, then GitHub
        local webhookCode = nil
        local loadSource = "Unknown"

        -- Try to load from local file
        if isfile and readfile then
            local localPath = "webhook"
            if isfile(localPath) then
                webhookCode = readfile(localPath)
                loadSource = "Local File"
                print("üîç Loading webhook from local file...")
            end
        end

        -- If no local file, try GitHub
        if not webhookCode then
            print("üîç Loading webhook from GitHub...")
            local githubUrl = "https://raw.githubusercontent.com/bihyudyfosydf/Resn/refs/heads/main/Working"
            webhookCode = game:HttpGet(githubUrl)
            loadSource = "GitHub"
        end

        if webhookCode and #webhookCode > 0 then
            print("üìÑ Webhook code loaded from " .. loadSource .. " (" .. #webhookCode .. " characters)")
            local webhookInit = loadstring(webhookCode)()

            -- Check if we got a function that needs to be called
            if type(webhookInit) == "function" then
                print("üîß Executing webhook initialization function...")
                return webhookInit()
            else
                print("‚úÖ Webhook script executed directly")
                return true
            end
        else
            error("Empty response from " .. loadSource)
        end
    end)

    if success then
        print("‚úÖ RE:FACTOR Webhook Tab loaded successfully!")
        Library:Notify("üåê RE:FACTOR Webhook Tab integrated! Check the new tab.", 3)
    else
        print("‚ö†Ô∏è Could not load RE:FACTOR Webhook Tab:", tostring(result))
        Library:Notify("‚ö†Ô∏è RE:FACTOR Webhook Tab failed to load - check console for details", 3)

        -- Additional debug info
        print("üîç Debug: Make sure the webhook file exists at:")
        print("")
        print("üîç Debug: Error details:", tostring(result))

        -- Try loading local webhook file directly as fallback
        print("üîß Attempting direct local file load...")
        local directSuccess, directResult = pcall(function()
            dofile("webhook")
        end)

        if directSuccess then
            print("‚úÖ Webhook loaded via direct file method!")
            Library:Notify("üåê Webhook Tab loaded via fallback method!", 3)
        else
            print("‚ùå Direct file load also failed:", tostring(directResult))
        end
    end
end

-- Load Crafter Tab with a small delay to ensure everything is ready
task.spawn(function()
    task.wait(2) -- Wait 2 seconds for everything to initialize
    loadCrafterTab()
end)

-- Load Webhook Tab with a small delay after Crafter Tab
task.spawn(function()
    task.wait(4) -- Wait 4 seconds to ensure Crafter Tab loads first
    print("üåê Starting webhook tab initialization...")
    print("   Available variables:")
    print("   - Library:", tostring(Library ~= nil))
    print("   - Window:", tostring(Window ~= nil))
    print("   - Tabs:", tostring(Tabs ~= nil))
    loadWebhookTab()
end)

-- Auto-load Event Tab from GitHub or Local File
local function loadEventTab()
    local success, result = pcall(function()
        -- Ensure global variables are properly set for the event tab script
        if getgenv then
            getgenv().Library = Library
            getgenv().Window = Window
            getgenv().Tabs = Tabs
            getgenv().MainScriptLoaded = true
        end

        -- Make variables globally accessible without getgenv as backup
        _G.Library = Library
        _G.Window = Window
        _G.Tabs = Tabs
        _G.MainScriptLoaded = true

        -- Try local file first, then GitHub
        local eventCode = nil
        local loadSource = "Unknown"

        -- Try to load from local file
        if isfile and readfile then
            local localPath = "Onb"
            if isfile(localPath) then
                eventCode = readfile(localPath)
                loadSource = "Local File"
                print("üîç Loading event tab from local file...")
            end
        end

        -- If no local file, try GitHub
        if not eventCode then
            print("üîç Loading event tab from GitHub...")
            local githubUrl = "https://raw.githubusercontent.com/bihyudyfosydf/Resn/refs/heads/main/Onb"
            eventCode = game:HttpGet(githubUrl)
            loadSource = "GitHub"
        end

        if eventCode and #eventCode > 0 then
            print("üìÑ Event tab code loaded from " .. loadSource .. " (" .. #eventCode .. " characters)")
            local eventInit = loadstring(eventCode)()

            -- Check if we got a function that needs to be called
            if type(eventInit) == "function" then
                print("üîß Executing event tab initialization function...")
                return eventInit()
            else
                print("‚úÖ Event tab script executed directly")
                return true
            end
        else
            error("Empty response from " .. loadSource)
        end
    end)

    if success then
        print("‚úÖ RE:FACTOR Event Tab loaded successfully!")
        Library:Notify("üéÆ RE:FACTOR Event Tab integrated! Check the new tab.", 3)
    else
        print("‚ö†Ô∏è Could not load RE:FACTOR Event Tab:", tostring(result))
        Library:Notify("‚ö†Ô∏è RE:FACTOR Event Tab failed to load - check console for details", 3)

        -- Additional debug info
        print("üîç Debug: Make sure the event tab file exists at:")
        print("")
        print("üîç Debug: Error details:", tostring(result))

        -- Try loading local event file directly as fallback
        print("üîß Attempting direct local file load...")
        local directSuccess, directResult = pcall(function()
            dofile("Onb")
        end)

        if directSuccess then
            print("‚úÖ Event Tab loaded via direct file method!")
            Library:Notify("üéÆ Event Tab loaded via fallback method!", 3)
        else
            print("‚ùå Direct file load also failed:", tostring(directResult))
        end
    end
end

-- Load Event Tab with a small delay after Webhook Tab
task.spawn(function()
    task.wait(6) -- Wait 6 seconds to ensure Webhook Tab loads first
    print("üéÆ Starting event tab initialization...")
    print("   Available variables:")
    print("   - Library:", tostring(Library ~= nil))
    print("   - Window:", tostring(Window ~= nil))
    print("   - Tabs:", tostring(Tabs ~= nil))
    loadEventTab()
end)

-- ================================================================
-- SCRIPT COMPLETION MESSAGE
-- ================================================================
print("üå± REFACTOR Main Script - Fully Loaded with Auto Crafter, Webhook & Event Integration!")
Library:Notify("üöÄ All systems loaded! Crafter, Webhook & Event Tabs will appear in 2-6 seconds.", 4)
