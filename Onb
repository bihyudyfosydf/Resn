SummerStoreTab:AddButton("üõí Buy Selected Items Now", function()
    if not SelectedSummerItems or not next(SelectedSummerItems) then
        Library:Notify("‚ö†Ô∏è Please select summer items first!", 3)
        return
    end
    buySummerItems()
end)

